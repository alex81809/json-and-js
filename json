<!DOCTYPE html>
<html>
    <head>
        <title>Json</title>
    </head>

    <body>
        <h1 id="result"></h1>

        <script>

            var student = {
                name: "harry",
                class: "9",
                rank: 44
            }

            var convert = JSON.stringify(student)
            console.log(convert)

            var revert = JSON.parse(convert)

            document.getElementById("result").innerHTML = revert.name
        </script>
    </body>
</html>
